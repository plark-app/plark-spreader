{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "targets": {
                    "node": "current",
                    "uglify": true
                }
            }
        ]
    ],
    "retainLines": true,
    "plugins": [
        "dynamic-import-node",
        "@babel/plugin-transform-object-assign",
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        ["@babel/plugin-proposal-class-properties", { "loose" : true }],
        "@babel/plugin-proposal-object-rest-spread"
    ]
}